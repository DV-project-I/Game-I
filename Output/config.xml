<config>
  <app>
	  <title>Game Development template</title>
	  <maxFrameDuration value="16"/>
  </app>

  <renderer>
    <vsync value="true"/>
  </renderer>

  <window>
    <resolution width="1024" height="768" scale="3"/>
    <fullscreen value="false"/>
    <borderless value="false"/>
    <resizable value="false"/>
    <fullscreen_window value="false"/>
  </window>

  <audio>
    <music volume="128"/>
    <fx volume="128"/>
  </audio>

  <scene>
	<map path="Assets/Maps/" name="MapTemplate2.tmx"/>
    <player x="200" y="300" texturepath="Assets/personajes/SpritesheetParca/parca2.png"/>
    <item x="500" y="100" texturepath="Assets/personajes/Torrente/Torrente.png"/>
  
  </scene>

  
</config>
